<!-- /app/Resources/views/base.html.twig -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    {% block stylesheets %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/jquery-ui.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/jquery-ui.structure.min.css') }}">
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-table/1.12.2/bootstrap-table.min.css">
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/datepicker3.css')}}" >
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/bootstrap-datetimepicker.min.css')}}" >
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/styles.css')}}" >
    {% endblock %}
    <script src="https://code.jquery.com/jquery-2.0.2.min.js"></script>
    <script src="{{ asset('bundles/pylife/js/moment.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/moment-duration-format.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/moment-with-locales.min.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/jquery-ui.min.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/lumino.glyphs.js')}}"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-table/1.12.2/bootstrap-table.min.js"></script>
    <script src="{{ asset('bundles/pylife/js/bootstrap-datetimepicker.min.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/html5shiv.min.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/respond.min.js')}}"></script>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <title>{% block title %}PyramidLife{% endblock %}</title>

</head>

<body>

{% block header %}{% endblock %}
{% block sidebar %}{% endblock %}
{% block body %}{% endblock %}
{% block footer %}{% endblock %}

<!-- Bootstrap core JavaScript
================================================== -->
<!-- Placed at the end of the document so the pages load faster -->

{% block javascripts %}

{% endblock %}
</body>
</html>