<!-- /app/Resources/views/base.html.twig -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    {% block stylesheets %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/jquery-ui.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/jquery-ui.structure.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/bootstrap-table.css')}}" >
        <link rel="stylesheet" href="{{ asset('bundles/pylife/css/styles.css')}}" >
    {% endblock %}
    <script src="{{ asset('bundles/pylife/js/jquery-3.1.1.slim.min.js') }}"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="{{ asset('bundles/pylife/js/bootstrap-table.js')}}"></script>
    <script src="{{ asset('bundles/pylife/js/lumino.glyphs.js')}}"></script>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <title>{% block title %}PyramidLife{% endblock %}</title>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

    <![endif]-->
</head>

<body>

{% block header %}{% endblock %}
{% block sidebar %}{% endblock %}
{% block body %}{% endblock %}
{% block footer %}{% endblock %}

<!-- Bootstrap core JavaScript
================================================== -->
